{
    "version": 2,
    "env": {
        "PORT": "80"
    },
    "builds": [
        {
            "src": "dist/apps/api/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/apps/api/main.js",
            "methods": [
                "GET"
            ]
        }
    ]
}